{
  "name": "svitlo-power-ui",
  "private": true,
  "version": "0.0.0",
  "type": "module",
  "scripts": {
    "i18n:sync": "node -e \"require('fs-extra').copySync('../shared/i18n', 'src/i18n', { overwrite: true })\"",
    "i18n:watch": "chokidar \"../shared/i18n/**/*\" -c \"npm run i18n:sync\"",
    "dev": "concurrently \"npm run i18n:watch\" \"vite\"",
    "build": "npm run i18n:sync && tsc -b && vite build",
    "lint": "eslint .",
    "preview": "vite preview"
  },
